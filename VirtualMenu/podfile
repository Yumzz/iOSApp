use_frameworks!

# add the Firebase pod for Google Analytic

target 'VirtualMenu' do
#    pod 'Firebase/Analytics'
    pod 'Instructions'
    pod 'Firebase/Core', :inhibit_warnings => true
    pod 'Firebase/Database', :inhibit_warnings => true
    pod 'Firebase/Auth', :inhibit_warnings => true
    pod 'Firebase/Firestore'
    pod 'FirebaseFirestoreSwift'
    pod 'Firebase/Storage'
    pod 'GoogleSignIn', '5.0.0'
    pod 'PromiseKit', '6.15.3'
    pod 'MQTTClient'
    pod 'FBSDKLoginKit', '9.3.0'
    pod 'FBSDKCoreKit', '9.3.0'
    pod 'SquarePointOfSaleSDK'
end

#target 'YumzzAppClip' do
 #   pod 'Firebase'
#end


post_install do |installer|
  installer.generated_projects.each do |project|
    project.targets.each do |target|
      target.build_configurations.each do |config|
          config.build_settings['CLANG_WARN_QUOTED_INCLUDE_IN_FRAMEWORK_HEADER'] = 'NO'
      end
    end
  end
end

#target 'YumzzAppClip' do
 #    pod 'MQTTClient'
 #    pod 'SquarePointOfSaleSDK'
#end

target 'YumzzClip' do
 pod 'MQTTClient'
end


